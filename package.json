{
  "name": "svelte-bricks",
  "description": "Simple masonry implementation without column balancing",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-bricks",
  "repository": "https://github.com/janosh/svelte-bricks",
  "license": "MIT",
  "version": "0.3.1",
  "type": "module",
  "svelte": "./dist/index.js",
  "bugs": "https://github.com/janosh/svelte-bricks/issues",
  "dependencies": {
    "svelte": "^5.22.6"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.19.0",
    "@sveltejs/package": "^2.3.10",
    "jsdom": "^26.0.0",
    "mdsvex": "^0.12.3",
    "svelte-check": "^4.1.5",
    "svelte-preprocess": "^6.0.3",
    "svelte-toc": "^0.5.9",
    "svelte-zoo": "^0.4.16",
    "svelte2tsx": "^0.7.35",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  },
  "keywords": [
    "svelte",
    "masonry",
    "pinterest-layout",
    "component"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./Masonry.svelte": {
      "types": "./dist/Masonry.svelte.d.ts",
      "svelte": "./dist/Masonry.svelte",
      "default": "./dist/Masonry.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
