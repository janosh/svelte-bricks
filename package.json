{
  "name": "svelte-bricks",
  "description": "Simple masonry implementation without column balancing",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://github.com/janosh/svelte-bricks#readme",
  "repository": "https://github.com/janosh/svelte-bricks",
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "svelte": "Masonry.svelte",
  "bugs": {
    "url": "https://github.com/janosh/svelte-bricks/issues"
  },
  "scripts": {
    "dev": "svelte-kit dev",
